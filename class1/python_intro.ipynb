        "line1 = \"привет \"\n",
        "print(line1*4)\n",
        "\n",
        "# можно - сохранив все в переменные\n",
        "line1 = \"ну как дела \"\n",
        "count = 3\n",
        "result = line1*count #здесь мы умножили строку на число и сохранили результат в переменную result\n",
        "print(result)"
      ],
      "execution_count": 0,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
